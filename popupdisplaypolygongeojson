var layers = L.geoJson(states, style).addTo(map);


layers.on('mouseover', function(e) {
e.layer.setStyle({fillColor: 'red'});
e.layer.closePopup();
var feature = e.layer.feature;
var content = '<div><strong>' + feature.properties.NAME_1 + '</strong>' + '<p><em>' + feature.properties.RegionID + '</em></p></div>';
info.innerHTML = content; 
}).on('mouseout', function(e) {
e.layer.setStyle({fillColor:'yellow'});
});

</script>
<style>
.info{ position:absolute;top:20px;right: 490px;}
.info div{padding: 20px; background: #eeeeee; border: 1px solid black;}
</style>
</body>
</html>
